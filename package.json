{
	"name": "react-gallery",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"format": "prettier --write",
		"lint-and-fix": "eslint . --ext .ts --fix",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,css,scss}": [
			"prettier --write",
			"eslint --fix"
		],
		"*.{json,md,html}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@next/eslint-plugin-next": "^11.1.2",
		"next": "11.1.2",
		"node-sass": "^5.0.0",
		"react": "17.0.2",
		"react-dom": "17.0.2"
	},
	"devDependencies": {
		"@types/react": "17.0.20",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-next": "11.1.2",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.25.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^7.0.2",
		"lint-staged": "^11.1.2",
		"prettier": "^2.4.0",
		"typescript": "4.4.3"
	},
	"description": "Gallery component implemented in React",
	"main": "next.config.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chirayumit001/React-Gallery.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/chirayumit001/React-Gallery/issues"
	},
	"homepage": "https://github.com/chirayumit001/React-Gallery#readme"
}
